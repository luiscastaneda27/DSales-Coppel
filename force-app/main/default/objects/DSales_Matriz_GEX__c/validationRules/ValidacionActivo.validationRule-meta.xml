<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidacionActivo</fullName>
    <active>true</active>
    <errorConditionFormula>AND( DSALES_EsClone__c,  DSALES_Activo__c )</errorConditionFormula>
    <errorMessage>Una matriz clonada no puede estar activa</errorMessage>
</ValidationRule>
